<!--Add Popup-->
<div id="companyUpdatePartial" class="modal fade" aria-hidden="true">
    <div class="modal-dialog" style="width: 580px;z-index:10052!important;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title danger">Add Fixture</h4>
            </div>
            <div class="scroller" style="height: 370px" data-always-visible="0" data-rail-visible1="1">
                <div class="modal-body">
                    <form class="form-horizontal ng-pristine ng-invalid ng-invalid-required" name="FixtureForm">
                        <div class="col-md-6">
                            <span>Name</span>
                            <input ng-model="Pop.Name" style="font-size: 14px;" type="text" class="form-control" required>
                        </div>
                   

                    </form>
                </div>
                <div class="modal-footer" style="margin-top:43%;">
                    <input type="hidden" />
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn red" ng-disabled="FixtureForm.$invalid" ng-click="AddFixture()">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>