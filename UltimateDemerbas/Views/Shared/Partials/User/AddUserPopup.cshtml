
<div id="AddUser" class="modal fade" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog" style="width: 800px;z-index:10052!important;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title danger">Add New User</h4>
            </div>
            <div class="scroller" style="height:350px" data-always-visible="0" data-rail-visible1="1">
                <div class="modal-body">
                    <form name="AddUserForm" class="col-md-12">

                        <div class="form-group form-md-line-input has-success col-md-6 pull-left">
                            <label class="col-md-3 control-label" for="form_control_1">Name</label>
                            <div class="col-md-9" >
                                <input type="text" name="Name" class="form-control" ng-model="test" placeholder="" required>
                                <div class="font-red-mint" ng-show="AddUserForm.test.$pristine"><i>Name field is required</i></div>
                            </div>
                        </div>
                        <div class="form-group form-md-line-input has-success col-md-6 pull-right">
                            <label class="col-md-3 control-label" for="form_control_1">Surname</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" ng-model="tester" placeholder="" required>
                                <div class="font-red-mint" ng-show="AddUserForm.tester.$invalid"><i>Surname field is required</i></div>
                            </div>
                        </div>


                        <div class="form-group form-md-line-input has-success col-md-6 pull-left">
                            <label class="col-md-3 control-label" for="form_control_1">Username</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" ng-model="Pop.Username" placeholder="" required>
                                <div class="font-red-mint" ng-show="AddUserForm.Username.$invalid"><i>Username field is required</i></div>

                            </div>
                        </div>

                        <div class="form-group form-md-line-input has-success col-md-6 pull-right">
                            <label class="col-md-3 control-label" for="form_control_1">Password</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" ng-model="Pop.Password" placeholder="" required>
                                <div class="font-red-mint" ng-show="AddUserForm.Password.$invalid"><i>Password field is required</i></div>
                            </div>
                        </div>

                        <div class="form-group form-md-line-input has-success col-md-6 pull-left">
                            <label class="col-md-3 control-label" for="form_control_1">Try Password</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" ng-model="Pop.TryPassword" placeholder="" required>
                                <div class="font-red-mint" ng-show="AddUserForm.TryPassword.$invalid"><i>TryPassword field is required</i></div>
                            </div>
                        </div>

                        <div class="form-group form-md-radios has-success col-md-6 pull-right">
                            <label for="form_control_1">Gender</label>
                            <div class="md-radio-inline">
                                <div class="md-radio">
                                    <input type="radio" id="checkbox2_8" name="radio2" class="md-radiobtn">
                                    <label for="checkbox2_8">
                                        <span></span>
                                        <span class="check"></span>
                                        <span class="box"></span> Women
                                    </label>
                                </div>
                                <div class="md-radio">
                                    <input type="radio" id="checkbox2_9" name="radio2" class="md-radiobtn" checked="">
                                    <label for="checkbox2_9">
                                        <span></span>
                                        <span class="check"></span>
                                        <span class="box"></span> Man
                                    </label>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>
                <div class="modal-footer" >
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn red" ng-click="AddLicense()">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>
