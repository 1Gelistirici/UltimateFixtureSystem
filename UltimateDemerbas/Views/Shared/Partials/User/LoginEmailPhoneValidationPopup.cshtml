
<div id="AddUser" @*class="modal fade" aria-hidden="true"*@>
    <div class="modal-dialog" style="width: 500px;z-index:10052!important;">
        <div class="modal-content">
            <div class="scroller" style="height:350px" data-always-visible="0" data-rail-visible1="1">
                <div class="modal-body">
                    <form name="AddUserForm" class="col-md-12">


                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn dark btn-outline" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn red" ng-disabled="AddUserForm.$invalid" ng-click="AddUser()">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>
